generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model EditJob {
  id String @id @default(uuid())
  inputUrl String
  outputUrl String?
  prompt String
  parsedCommand Json
  status JobStatus @default(PENDING)
}

enum JobStatus {
  PENDING
  PROCESSING
  COMPLETED
  FAILED
}